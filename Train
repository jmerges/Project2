f = load('mnist_all.mat');
n=Network(f.train0(1,1:784),2);
n=Reverse_Pass(n,f.train0(1,:),1);

for k = 1:500
    for j = 1:9
        target=0;
        for i = 1:2
            n=Test(n,f.train0(i*500-499,:),1);
            n=Reverse_Pass(n,f.train0(i*500-499,:),1);
        end
        for i = 1:2
            n=Test(n,f.(sprintf('train%d', j))(i*500-499,:),target);
            n=Reverse_Pass(n,f.(sprintf('train%d', j))(i*500-499,:),target);
            disp([i k j]);
        end
    end
end
